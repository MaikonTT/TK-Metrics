{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "InfluxDbConfiguration": {
    "Url": "http://localhost:8086/",
    "Database": "tkmetrics",
    "User": "admin",
    "Password": "admin123"
  },
  "MetricsOptions": {
    "DefaultContextLabel": "api-tk-metrics",
    "GlobalTags": {
      "env": "local"
    },
    "Enabled": true
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.5,
    "IgnoredHttpStatusCodes": [ 404 ],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": false
  },
  "MetricsEndpointsOptions": {
    "MetricsEndpointEnabled": false,
    "MetricsTextEndpointEnabled": false,
    "EnvironmentInfoEndpointEnabled": false
  },
  "MetricMiddlewareConfiguration": [
    {
      "RouteName": "Employee",
      "RoutePath": "employeeregister",
      "RouteSla": "2000"
    },
    {
      "RouteName": "Product",
      "RoutePath": "productregister",
      "RouteSla": "2000"
    }
  ]
}
